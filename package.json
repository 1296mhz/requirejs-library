{
  "name": "requirejs-library",
  "version": "1.0.0",
  "homepage": "http://github.com/sahat/requirejs-library",
  "description": "Library skeleton project using require.js",
  "scripts": {
    "test": "./node_modules/.bin/karma start --single-run --browsers PhantomJS"
  },
  "jshintConfig": {
    "quotmark": "single",
    "boss": true,
    "node": true,
    "strict": true,
    "white": true,
    "smarttabs": true,
    "undef": true,
    "unused": true,
    "onecase": true,
    "lastsemic": true,
    "newcap": false,
    "maxlen": 80,
    "indent": 2
  },
  "devDependencies": {
    "karma": "~0.10.9",
    "karma-mocha": "~0.1.1",
    "grunt": "~0.4.2",
    "grunt-contrib-requirejs": "~0.4.1",
    "grunt-contrib-watch": "~0.5.3"
  }
}
